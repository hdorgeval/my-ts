{
  "name": "my-ts",
  "version": "1.0.6",
  "description": "fluent API for TypeScript",
  "main": "build/my.js",
  "typings": "build/my.d.ts",
  "scripts": {
    "pretest": "tsc",
    "test": "jest --runInBand --coverage --verbose",
    "preupdate:all": "rimraf ./node_modules",
    "update:all": "npm run update:cli && npm run update:packages && npm run update:types",
    "update:packages": "npm install --save typescript@latest && npm install --save-dev jest@latest tslint@latest",
    "update:types": "npm install --save-dev @types/jest @types/node",
    "update:cli": "npm install -g typescript@latest jest@latest tslint@latest rimraf@latest",
    "update:check": "npm outdated",
    "update:npm": "npm -v && npm install -g npm@latest && npm -v",
    "lint": "tslint 'src/**/*.ts'",
    "package": "npm run lint && rimraf ./build && rimraf ./package && rm -f my*.tgz && tsc -p tsconfig.npm-pack.json --listEmittedFiles && jest --config=jestconfig.npm-pack.json && npm pack"
  },
  "author": "Henri d'Orgeval",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "19.2.3",
    "@types/node": "7.0.18",
    "jest": "20.0.3",
    "tslint": "5.2.0"
  },
  "dependencies": {
    "typescript": "2.3.2"
  },
  "engines": {
    "node": ">=7.0.8",
    "npm": ">=4.2.0"
  },
  "jest": {
    "roots": [
      "./src/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hdorgeval/my-ts.git"
  },
  "keywords": [
    "fluent",
    "API",
    "TypeScript",
    "library"
  ],
  "bugs": {
    "url": "https://github.com/hdorgeval/my-ts/issues"
  },
  "homepage": "https://github.com/hdorgeval/my-ts#readme"
}
